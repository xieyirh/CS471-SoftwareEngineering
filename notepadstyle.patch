+			<!-- Keywords that should be operators -->
+			<KEYWORD2>is</KEYWORD2>
+			<KEYWORD2>rem</KEYWORD2>
+			<KEYWORD2>mod</KEYWORD2>
+
+			<!-- Singleton variable -->
+			<KEYWORD3>_</KEYWORD3>
+
+		</KEYWORDS>
+
+	</RULES>
+
+	<RULES SET="LIST" DEFAULT="LITERAL2" ESCAPE="\">
+		<SPAN TYPE="LITERAL2" NO_LINE_BREAK="TRUE" DELEGATE="LIST">
+			<BEGIN>[</BEGIN>
+			<END>]</END>
+		</SPAN>
+	</RULES>
