+            <KEYWORD2>resident</KEYWORD2>
+            <KEYWORD2>safecall</KEYWORD2>
+            <KEYWORD2>stdcall</KEYWORD2>
+            <KEYWORD2>stored</KEYWORD2>
+            <KEYWORD2>varargs</KEYWORD2>
