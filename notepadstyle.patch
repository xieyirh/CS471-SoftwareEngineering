+		<EOL_SPAN TYPE="COMMENT2">//</EOL_SPAN>
+		<EOL_SPAN TYPE="COMMENT4">#</EOL_SPAN>
+
+		<!-- back to MAIN for "?>" -->
+		<SEQ TYPE="MARKUP" DELEGATE="MAIN">?&gt;</SEQ>
+
+		<!-- Literals: -->
+		<SPAN TYPE="LITERAL1" ESCAPE="\">
+			<BEGIN>'</BEGIN>
+			<END>'</END>
+		</SPAN>
+		<SPAN TYPE="LITERAL2" DELEGATE="PHP_LITERAL2">
+			<BEGIN>"</BEGIN>
+			<END>"</END>
+		</SPAN>
