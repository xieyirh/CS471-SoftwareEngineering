+	<!-- Empty ruleset to delegate to for spots where any non-whitespace is invalid -->
+	<RULES SET="INVALID" DEFAULT="INVALID">
+		<!-- Limit the error to one line -->
+		<EOL_SPAN_REGEXP TYPE="INVALID">\S.*</EOL_SPAN_REGEXP>
+	</RULES>
