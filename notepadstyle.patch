+		  <LABEL>@dl</LABEL>
+		  <LABEL>@enddl</LABEL>
+		  <LABEL>@ol</LABEL>
+		  <LABEL>@endol</LABEL>
+		  <LABEL>@ul</LABEL>
+		  <LABEL>@endol</LABEL>
+
+		  <!-- Special keywords -->
+		  <LABEL>@ignore</LABEL>
+		  <LABEL>@endignore</LABEL>
+
+		</KEYWORDS>
+
+	</RULES>
+
