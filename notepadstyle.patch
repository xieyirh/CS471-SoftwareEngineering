+		<SEQ TYPE="FUNCTION" AT_WORD_START="TRUE">and(</SEQ>
+
+		<!-- Concatenation operators -->
+		<!-- strings -->
+		<SEQ TYPE="OPERATOR">&amp;</SEQ>
+
+		<!--		<SEQ TYPE="NULL">_</SEQ>-->
+		<!-- Instruction separator -->
+		<MARK_PREVIOUS TYPE="LABEL" MATCH_TYPE="OPERATOR"
+			AT_LINE_START="TRUE">:</MARK_PREVIOUS>
+
+		<KEYWORDS>
+
+			<!--  just testing! -->
