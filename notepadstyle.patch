+	</PROPS>
+
+	<RULES ESCAPE="\" IGNORE_CASE="TRUE" HIGHLIGHT_DIGITS="TRUE">
+		<SPAN TYPE="LITERAL1" NO_LINE_BREAK="TRUE">
+			<BEGIN>"</BEGIN>
+			<END>"</END>
+		</SPAN>
+
+      <!-- Comments -->
+		<EOL_SPAN TYPE="COMMENT4">'</EOL_SPAN>
+		<EOL_SPAN TYPE="COMMENT4">rem </EOL_SPAN>
