+      <END REGEXP="TRUE">\s+</END>
+    </SPAN_REGEXP>
+
+    <!-- Display Strings. -->
+    <SPAN_REGEXP TYPE="KEYWORD1"
+                 AT_WORD_START="TRUE"
+                 DELEGATE="STRING">
+      <BEGIN>[.]\(($|\s+)</BEGIN>
+      <END REGEXP="TRUE">(?=\))</END>
+    </SPAN_REGEXP>
+
+    <!-- Character Literals. -->
+    <SPAN_REGEXP TYPE="KEYWORD1"
+                    AT_WORD_START="TRUE"
+                    NO_LINE_BREAK="TRUE"
+                    DELEGATE="LITERAL">
