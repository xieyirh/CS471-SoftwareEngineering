+		<!-- Logical (*): -->
+		<SEQ TYPE="OPERATOR">&amp;&amp;</SEQ>  <!-- and -->
+		<SEQ TYPE="OPERATOR">||</SEQ>  <!-- or -->
+		<SEQ TYPE="OPERATOR">!</SEQ>   <!-- unary negation -->
+
+		<!-- Assignment (*): -->
+		<SEQ TYPE="OPERATOR">=</SEQ>   <!-- (to register variables a..z) -->
+
+
+		<SEQ TYPE="LITERAL1">$</SEQ>
+		<SEQ TYPE="LITERAL1">@</SEQ>
+		<SEQ TYPE="LITERAL1">#</SEQ>
+		<SEQ TYPE="LITERAL1">*</SEQ>
+		<SEQ TYPE="LITERAL1">{</SEQ>
+		<SEQ TYPE="LITERAL1">}</SEQ>
+
+		<KEYWORDS>
