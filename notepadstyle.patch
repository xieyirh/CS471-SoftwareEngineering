+    <!-- If we spot something that looks like an AVT, assume it is. Lazy, I know... -->
+    <SPAN TYPE="KEYWORD2" DELEGATE="AVT">
+      <BEGIN>"</BEGIN>
+      <END>"</END>
+    </SPAN>
+    <SPAN TYPE="KEYWORD2" DELEGATE="AVT">
+      <BEGIN>'</BEGIN>
+      <END>'</END>
+    </SPAN>
