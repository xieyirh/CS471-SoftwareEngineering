+			<BEGIN>'</BEGIN>
+			<END>'</END>
+		</SPAN>
+
+		<SPAN TYPE="LITERAL1" NO_LINE_BREAK="TRUE" DELEGATE="STRING">
+			<BEGIN>"</BEGIN>
+			<END>"</END>
+		</SPAN>
+
+        <SPAN TYPE="LITERAL1" NO_LINE_BREAK="TRUE" DELEGATE="STRING">
+			<BEGIN>`</BEGIN>
+			<END>`</END>
+		</SPAN>
+
+        <SPAN TYPE="LITERAL1" DELEGATE="LIST">
+			<BEGIN>[</BEGIN>
+			<END>]</END>
+		</SPAN>
+
+        <SPAN TYPE="LITERAL1" DELEGATE="LIST">
+			<BEGIN>{</BEGIN>
+			<END>}</END>
+		</SPAN>
+
+        <SPAN TYPE="LITERAL1" DELEGATE="LIST">
+			<BEGIN>![</BEGIN>
+			<END>]</END>
+		</SPAN>
+
