+
+// Process
+void lookAndFeelProperties()
+{
+	defaults = javax.swing.UIManager.getLookAndFeelDefaults();
+	sb = new StringWriter();
+	Properties p = new Properties();
+	for(Iterator it=defaults.entrySet().iterator();it.hasNext();){
