+			<LITERAL3>DATE</LITERAL3>
+			<LITERAL3>DURATION</LITERAL3>
+		</KEYWORDS>
+	</RULES>
+
+	<!-- Custom keywords -->
+	<RULES SET="CUSTOM_KEYWORDS" IGNORE_CASE="FALSE">
+		<SEQ_REGEXP HASH_CHAR="X" AT_LINE_START="TRUE" TYPE="INVALID">X-[A-Za-z-]+</SEQ_REGEXP>
+	</RULES>
+</MODE>
