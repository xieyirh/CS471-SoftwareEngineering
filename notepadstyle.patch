+			<BEGIN>"</BEGIN>
+			<END>"</END>
+		</SPAN>
+		<SPAN TYPE="LABEL">
+			<BEGIN>'</BEGIN>
+			<END>'</END>
+		</SPAN>
+
+		<SEQ TYPE="OPERATOR">*</SEQ>
+
+
+		<KEYWORDS>
+			<KEYWORD1>include</KEYWORD1>
+			<OPERATOR>=</OPERATOR>
+			<KEYWORD2>START</KEYWORD2>
+			<KEYWORD2>END</KEYWORD2>
+		</KEYWORDS>
+	</RULES>
+
+	<RULES SET="TAGS" DEFAULT="MARKUP" IGNORE_CASE="TRUE" HIGHLIGHT_DIGITS="TRUE">
+		<SPAN TYPE="LITERAL1">
+			<BEGIN>"</BEGIN>
+			<END>"</END>
+		</SPAN>
+		<SPAN TYPE="LITERAL1">
+			<BEGIN>'</BEGIN>
+			<END>'</END>
+		</SPAN>
+
+		<SEQ TYPE="OPERATOR">=</SEQ>
+
+	</RULES>
+</MODE>
