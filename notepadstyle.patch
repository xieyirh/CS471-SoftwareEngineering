+      <END REGEXP="TRUE">\s+</END>
+    </SPAN_REGEXP>
+
+    <!-- Obsolete Keywords. -->
+    <SPAN_REGEXP TYPE="INVALID"
+                    AT_WORD_START="TRUE"
+                    NO_LINE_BREAK="TRUE"
+                    DELEGATE="WORD">
