+	<!--{{{ rule PHP_COMMENTS -->
+	<!-- Comments in PHP -->
+	<RULES SET="PHP_COMMENTS">
+		<!-- special case -->
+		<SEQ TYPE="COMMENT1">/**/</SEQ>
+
+		<!-- PHPdoc comment -->
+		<SPAN TYPE="COMMENT3" DELEGATE="PHPDOC">
+			<BEGIN>/**</BEGIN>
+			<END>*/</END>
+		</SPAN>
+
+		<!-- range comment -->
+		<SPAN TYPE="COMMENT1">
+			<BEGIN>/*</BEGIN>
+			<END>*/</END>
+		</SPAN>
+
+		<!-- line comments -->
+		<EOL_SPAN TYPE="COMMENT2">//</EOL_SPAN>
+		<EOL_SPAN TYPE="COMMENT4">#</EOL_SPAN>
+	</RULES>
