+<?xml version="1.0"?>
+
+<!DOCTYPE MODE SYSTEM "xmode.dtd">
+
+<!--
+<MODE NAME="svn-commit"		FILE="svn-commit.xml"/>
+-->
+
+<MODE>
+	<PROPS>
